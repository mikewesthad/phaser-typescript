{
  "name": "phaser-typescript",
  "version": "1.0.0",
  "description": "Phaser & TypeScript Template",
  "main": "src/index.ts",
  "scripts": {
    "build": "webpack --mode production --config ./config/webpack.config.js",
    "dev": "webpack server --mode development --open --config ./config/webpack.config.js",
    "prettier": "prettier --write ./src/js/**/*.{js,jsx,json}",
    "push:gh-pages": "gh-pages --dist dist --dest .",
    "predeploy": "npm run build",
    "deploy": "npm run push:gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikewesthad/phaser-typescript.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikewesthad/phaser-typescript/issues"
  },
  "homepage": "https://github.com/mikewesthad/phaser-typescript#readme",
  "prettier": {
    "printWidth": 100
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "prettier": "^2.3.0",
    "ts-loader": "^9.2.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "phaser": "^3.54.0"
  }
}
